CREATE MEMORY TABLE PROJECTMEMBER(MEMBERID VARCHAR(255) NOT NULL PRIMARY KEY,NICKNAME VARCHAR(100),TRUSTLEVEL INTEGER)
CREATE MEMORY TABLE FILEOBJECT(CHECKSUM VARCHAR(255),FILESIZE BIGINT,LOCALLYMODIFIED BOOLEAN NOT NULL,RELPATH VARCHAR(255) NOT NULL,OBJECTID CHAR(36) NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE JAKEOBJECT(OBJECTID CHAR(36) NOT NULL PRIMARY KEY,DELETED BOOLEAN,MODIFIED BOOLEAN,CONSTRAINT SYS_CT_48 UNIQUE(OBJECTID))
CREATE MEMORY TABLE LOGENTRIES(DTYPE VARCHAR(31) NOT NULL,ID VARCHAR(255) NOT NULL PRIMARY KEY,BELONGSTO LONGVARBINARY,HASH VARCHAR(255),COMMENT VARCHAR(255),ACTION INTEGER NOT NULL,OBJECTUUID VARCHAR(255),PROCESSED BOOLEAN NOT NULL,PROJECT CHAR(36),TIME TIMESTAMP NOT NULL,MEMBER_MEMBERID VARCHAR(255),CONSTRAINT FKC5636CCC6B508D87 FOREIGN KEY(MEMBER_MEMBERID) REFERENCES PROJECTMEMBER(MEMBERID))
CREATE MEMORY TABLE NOTE(TEXT VARCHAR(255) NOT NULL,OBJECTID CHAR(36) NOT NULL PRIMARY KEY,CONSTRAINT FK33AFF2E0E1910 FOREIGN KEY(OBJECTID) REFERENCES JAKEOBJECT(OBJECTID))
CREATE MEMORY TABLE PROJECT(UUID VARCHAR(255) NOT NULL PRIMARY KEY,AUTOANNOUNCE BOOLEAN NOT NULL,AUTOPULL BOOLEAN NOT NULL,AUTOLOGIN BOOLEAN NOT NULL,INVITATIONSTATE INTEGER,NAME VARCHAR(255) NOT NULL,OPENED BOOLEAN NOT NULL,ROOTPATH VARCHAR(255) NOT NULL,STARTED BOOLEAN NOT NULL,USERID VARCHAR(255),CONSTRAINT SYS_CT_53 UNIQUE(UUID))
CREATE MEMORY TABLE SERVICECREDENTIALS(UUID VARCHAR(255) NOT NULL PRIMARY KEY,AUTOLOGIN BOOLEAN,ENCRYPTION BOOLEAN NOT NULL,PASSWORD VARCHAR(255),PROTOCOL VARCHAR(255),RESOURCENAME VARCHAR(255) NOT NULL,SAVEPASSWORD BOOLEAN,SERVER VARCHAR(255) NOT NULL,PORT BIGINT NOT NULL,USERNAME VARCHAR(255) NOT NULL)
CREATE MEMORY TABLE TAG(TEXT VARCHAR(255) NOT NULL,OBJECTID CHAR(36) NOT NULL,PRIMARY KEY(TEXT,OBJECTID),CONSTRAINT FK1BF9AE0E1910 FOREIGN KEY(OBJECTID) REFERENCES JAKEOBJECT(OBJECTID))
CREATE MEMORY TABLE USERS(PROTOCOL VARCHAR(31) NOT NULL,UUID VARCHAR(255) NOT NULL PRIMARY KEY,SC_UUID VARCHAR(255),FIRSTNAME VARCHAR(255),NICKNAME VARCHAR(255),SURNAME VARCHAR(255),USERID VARCHAR(255))
ALTER TABLE FILEOBJECT ADD CONSTRAINT FK9A298BBBE0E1910 FOREIGN KEY(OBJECTID) REFERENCES JAKEOBJECT(OBJECTID)
ALTER TABLE PROJECT ADD CONSTRAINT FKED904B19954BA233 FOREIGN KEY(USERID) REFERENCES SERVICECREDENTIALS(UUID)
